{
  "name": "@zazen/stylelint-config",
  "version": "2.0.0",
  "description": "Lint CSS, free of weariness and confusion",
  "keywords": [
    "stylelint-config"
  ],
  "homepage": "https://github.com/stormwarning/zazen-stylelint-config",
  "repository": "stormwarning/zazen-stylelint-config",
  "license": "ISC",
  "author": "Jeff (https://tidaltheory.io)",
  "scripts": {
    "changeset": "changeset add",
    "lint": "eslint '**/*.js'",
    "release": "changeset publish",
    "test": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "stylelint-config-recess-order": "2.3.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-order": "4.1.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.2.8",
    "@changesets/cli": "2.14.x",
    "@zazen/eslint-config": "2.2.x",
    "eslint": "7.21.x",
    "husky": "4.3.x",
    "lint-staged": "10.5.x",
    "prettier": "2.2.x",
    "prettier-plugin-packagejson": "2.2.x"
  },
  "peerDependencies": {
    "stylelint": ">=9"
  },
  "changelog": {
    "repo": "stormwarning/zazen-eslint-config",
    "labels": {
      "breaking": "ğŸ’£ Breaking Change",
      "removed": "ğŸ—‘ï¸ Removed",
      "deprecated": "ğŸšš Deprecated",
      "changed": "â™»ï¸ Changed",
      "added": "ğŸ Added",
      "fixed": "ğŸ› Fixed",
      "security": "ğŸ”’ Security"
    }
  }
}
