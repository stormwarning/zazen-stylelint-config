{
  "name": "@zazen/stylelint-config",
  "version": "2.0.0",
  "description": "Lint CSS, free of weariness and confusion",
  "keywords": [
    "stylelint-config"
  ],
  "homepage": "https://github.com/stormwarning/zazen-stylelint-config",
  "repository": "stormwarning/zazen-stylelint-config",
  "license": "ISC",
  "author": "Jeff (https://tidaltheory.io)",
  "scripts": {
    "changeset": "changeset add",
    "lint": "eslint '**/*.js'",
    "prepare": "husky install",
    "release": "changeset publish",
    "test": "npm run lint"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-recommended": "7.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.2.8",
    "@changesets/cli": "2.22.0",
    "@zazen/eslint-config": "5.1.0",
    "eslint": "8.15.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "prettier-plugin-packagejson": "2.2.17"
  },
  "peerDependencies": {
    "stylelint": ">=14"
  },
  "changelog": {
    "repo": "stormwarning/zazen-stylelint-config",
    "labels": {
      "breaking": "💣 Breaking Change",
      "removed": "🗑️ Removed",
      "deprecated": "🚚 Deprecated",
      "changed": "♻️ Changed",
      "added": "🎁 Added",
      "fixed": "🐛 Fixed",
      "security": "🔒 Security"
    }
  }
}
